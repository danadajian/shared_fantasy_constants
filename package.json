{
  "name": "@dadajian/shared-fantasy-constants",
  "version": "1.1.2",
  "description": "Shared constants for fantasy sports apps",
  "main": "lib/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danadajian/shared_fantasy_constants.git"
  },
  "author": "Dan Adajian",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-sdk": "latest",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.8",
    "aws-sdk": "latest",
    "dotenv": "^8.2.0",
    "jest": "^26.2.2",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
